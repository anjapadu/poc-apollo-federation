{
  "name": "gateway",
  "private": true,
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && mkdir -p ./dist/schema && [ -d ./src/schema ] && cp -r ./src/schema/* ./dist/schema/ || true",
    "build:prod": "tsc -p tsconfig.prod.json && mkdir -p ./dist/schema && [ -d ./src/schema ] && cp -r ./src/schema/* ./dist/schema/ || true",
    "watch": "tsc --watch",
    "start:watch": "node watch.js",
    "dev": "concurrently -k \"yarn watch\" \"yarn start:watch\""
  },
  "dependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.2",
    "@as-integrations/aws-lambda": "^3.1.0",
    "graphql": "^16.10.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
